<templateSet group="JavaScript">
  <template name="us" value="'use strict';&#10;$END$" description="Inserts 'use strict' statement" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
  <template name="desc" value="describe('$VAR$', function () {&#10;    $END$&#10;});" description="Jasmine `describe` block" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fn" value="function () {$END$}" description="function () {}" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fna" value="function ($VAR$) {&#10;    $END$&#10;}" description="function with args" toReformat="true" toShortenFQNames="true">
